<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="689.0" prefWidth="752.0" xmlns="http://javafx.com/javafx/8.0.261" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MainController">
   <children>
      <HBox styleClass="titleback">
         <children>
            <HBox fx:id="textFieldSectionHBox" HBox.hgrow="ALWAYS">
               <children>
                  <Button fx:id="settingsButton" mnemonicParsing="false" onAction="#toggleSettings" prefHeight="36.0" prefWidth="89.0" styleClass="clean-button" text="Settings" />
                  <TextField fx:id="queryTextField" onAction="#findTermsMatchingTheProvidedQuery" promptText="What's your question? (Ctrl+L)" HBox.hgrow="ALWAYS" />
                  <Button fx:id="searchButton" mnemonicParsing="false" onAction="#findTermsMatchingTheProvidedQuery" prefHeight="36.0" prefWidth="89.0" styleClass="clean-button" text="Search" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <VBox VBox.vgrow="ALWAYS">
         <children>
            <StackPane alignment="BOTTOM_CENTER" VBox.vgrow="ALWAYS">
               <children>
                  <StackPane fx:id="stackpane">
                     <children>
                        <ScrollPane fitToHeight="true" fitToWidth="true" styleClass="edge-to-edge">
                           <content>
                              <StackPane>
                                 <children>
                                    <VBox fx:id="foundTermsVBox" focusTraversable="true" maxWidth="1200.0" spacing="10.0" StackPane.alignment="CENTER">
                                       <padding>
                                          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                       </padding>
                                    </VBox>
                                    <Label fx:id="oldInfoLabel" text="For full results, sign in to Quizlet on your browser" />
                                 </children>
                              </StackPane>
                           </content>
                        </ScrollPane>
                     </children>
                     <StackPane.margin>
                        <Insets />
                     </StackPane.margin>
                  </StackPane>
                  <TitledPane fx:id="summaryInformationTab" alignment="TOP_LEFT" animated="false" expanded="false">
                     <content>
                        <ScrollPane fitToWidth="true">
                           <content>
                              <VBox fx:id="summaryInformationVBox" spacing="14.0">
                                 <padding>
                                    <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
                                 </padding>
                              </VBox>
                           </content>
                        </ScrollPane>
                     </content>
                     <graphic>
                        <HBox fx:id="titledPaneHBoxContainer">
                           <children>
                              <Label disable="true" text="Search Summary Information" />
                              <Region HBox.hgrow="ALWAYS" />
                              <Label fx:id="totalElapsedTimeLabel" />
                           </children>
                        </HBox>
                     </graphic>
                     <StackPane.margin>
                        <Insets />
                     </StackPane.margin>
                  </TitledPane>
               </children>
            </StackPane>
         </children>
      </VBox>
   </children>
</VBox>
